	<Element>
		<NameOfElement>Options</NameOfElement>
		<ElementProperty>
			<Type>id</Type>
			<Value>org.wikipedia:id/menu_icon</Value>
		</ElementProperty>
	</Element>
	<Element>
		<NameOfElement>LoginTab</NameOfElement>
		<ElementProperty>
